    ".react-router": true,
    ".swc": true,
    "Dockerfile": true,
    "README.md": true,
    // "app/root.jsx": true,
    "build": true,
    "docs": true,
    "eslint.config.js.bak": true,
    "jsconfig.json": true,
    "neutralinojs.log": true,
    "node_modules": true,
    "pnpm-lock.yaml": true,
    "postcss.config.js": true,
    "prisma.config.js": true,
    "public": true,
    "react-router.config.js": true,
    "eslint.config.js": true,
    "scripts": true,
    "server.js": true,
    "app/entry.client.jsx": true,
    "app/entry.server.jsx": true,
